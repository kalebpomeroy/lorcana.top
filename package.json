{
  "name": "lorcana.top",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "start": "vue-cli-service build && node ./src/server.js",
    "lint": "vue-cli-service lint",
    "start:dev": "nodemon --watch ./src --exec 'npm start'",
    "dev": "concurrently \"npm run serve\" \"npm run start:dev\""
  },
  "dependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "axios": "^1.6.4",
    "chart.js": "^4.4.1",
    "core-js": "^3.8.3",
    "cors": "^2.8.5",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "vue": "^3.2.13",
    "vue-chartjs": "^5.3.0",
    "vue-lazyload": "^3.0.0",
    "vue-router": "^4.2.5",
    "vue3-lazy": "^1.0.0-alpha.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
